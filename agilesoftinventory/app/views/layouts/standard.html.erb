<script>
    
$(document).keypress(function(e) {
    if(e.which == 13) {
        e.preventDefault();
        var that = document.activeElement;
        $('[tabIndex=' + (+that.tabIndex + 1) + ']')[0].focus();       
    }
});
$("#res").click(function(){
$("#form")[0].reset();
$('#empid').focus();    
});   
    
$(document).ready(function(){
    $('#empid').focus();
});       
</script>
